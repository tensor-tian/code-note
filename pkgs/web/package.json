{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css": "npx tailwindcss -i ./src/input.css -o ./src/index.css --watch",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./src/index.css",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "prebuild": "npm run build:css && rm -rf ../vscode/dist/web",
    "postbuild": "mv ./build ../vscode/dist/web"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@code-hike-local/mdx": "workspace:*",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mdx-js/mdx": "2.3.0",
    "@mui/base": "5.0.0-beta.40",
    "@mui/material": "^5.15.19",
    "@react-spring/web": "^9.7.3",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "debug": "^4.3.5",
    "events": "^3.3.0",
    "javascript-time-ago": "^2.5.9",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-edit-text": "^5.1.1",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.1.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-split-pane": "^0.1.92",
    "react-time-ago": "^7.3.1",
    "react-use-keypress": "^1.3.1",
    "reactflow": "11.11.3",
    "redux": "^5.0.1",
    "rehype-katex": "^7.0.0",
    "remark-gfm": "3.0.1",
    "remark-math": "^6.0.0",
    "reselect": "^5.1.0",
    "types": "0.0.1",
    "usehooks-ts": "^3.1.0",
    "web-vitals": "^2.1.4",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.202",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/mdx": "^2.0.13",
    "@types/node": "^16.18.86",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/react-edit-text": "^5.0.4",
    "@types/vscode-webview": "^1.57.5",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.9.5"
  }
